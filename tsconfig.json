{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "types": [
      "vitepress/client"
    ]
  },
  "include": [
    ".vitepress/**/*",
    "types/**/*.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
